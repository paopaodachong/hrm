<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cjl.dao.EmployeeLevelMapper">

    <select id="getAllEmployeeLevel" resultType="com.cjl.model.EmployeeLevel">
        select * from T_EMPLOYEELEVEL
    </select>
    <select id="selectByEmployeeLevelId" resultType="com.cjl.model.EmployeeLevel">
        SELECT * from T_EMPLOYEELEVEL where EMPLOYEELEVEL_ID = #{employeeLevel_id}
    </select>
</mapper>
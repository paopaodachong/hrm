<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cjl.dao.AdminMapper">
    <select id="login" resultType="com.cjl.model.Admin">
        select * from T_ADMIN where ADMIN_NAME = #{admin_name} and ADMIN_PASS = #{admin_pass}
    </select>
</mapper>